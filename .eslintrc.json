{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-typescript-prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "max-classes-per-file": "off",
    "@typescript-eslint/no-non-null-assertion": "off",
    "no-restricted-syntax": "off"
  }
}
